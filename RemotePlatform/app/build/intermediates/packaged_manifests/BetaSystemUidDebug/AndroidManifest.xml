        <receiver
            android:name="com.coocaa.remoteplatform.baseability.abilities.reboot.RebootAlarmBroadcastReceiver"
            android:enabled="true"
            android:exported="true"
            android:process=":baseability" />

        <service
            android:name="com.coocaa.remoteplatform.baseability.BaseAbilityClient"
            android:exported="true"
            android:process=":baseability" >
            <meta-data
                android:name="id"
                android:value="com.coocaa.remoteplatform.baseability" />

            <intent-filter>
                <action android:name="coocaa.intent.action.remote.platform.client" />
            </intent-filter>
        </service>
        <service
            android:name="com.coocaa.libs.upgrader.runtime.service.UpgraderService"
            android:enabled="true"
            android:exported="true" >
            <intent-filter>
                <action android:name="coocaa.intent.action.UPGRADE_SERVICE" />

